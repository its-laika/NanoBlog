{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "PagePlaceholderPosts": "{POSTS}",
  "PagePlaceholderNavigation": "{NAVIGATION}",
  "PostPlaceholderContent": "{CONTENT}",
  "PostPlaceholderName": "{NAME}",
  "PostPlaceholderDate": "{DATE}",
  "PostDateFormat": "yyyy-MM-dd HH:mm K",
  "PageTemplate": "<html><body>{POSTS}<br>{NAVIGATION}</body></html>",
  "PostTemplate": "<div class='post' id='{NAME}'>{CONTENT}</div>",
  "UsePagination": true,
  "PageSize": 20,
  "PostDirectory": "blog-files/posts",
  "AssetDirectory": "blog-files/assets",
  "ExportDirectory": "blog-export",
  "KeepExportFiles": [],
  "AuthenticationToken": null
}